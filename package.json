		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"handlebars": "^4.7.7",
		"jest": "^29.3.1",
		"obsidian": "latest",
		"standard-version": "^9.5.0",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
